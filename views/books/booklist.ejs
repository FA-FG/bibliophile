<body>
  <h1>Bibliophile</h1>
  <h1>User Book List</h1>
  <div class="book-list">
    <% books.forEach(book => { %>
      <div class="book">
        <img src="<%= book.image %>" alt="<%= book.bookName %>">
        <div class="book-details">
          <h3><%= book.bookName %></h3>
          <p><strong>Reading Status:</strong> <%= book.readingStatus %></p>
        </div>
      </div>
    <% }); %>
  </div>
 
</body>